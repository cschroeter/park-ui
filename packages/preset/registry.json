        },
        {
          "path": "src/theme/animation-styles.ts",
          "type": "registry:theme"
        },
        {
          "path": "src/theme/conditions.ts",
          "type": "registry:theme"
        },
        {
          "path": "src/theme/global-css.ts",
          "type": "registry:theme"
        },
        {
          "path": "src/theme/keyframes.ts",
          "type": "registry:theme"
        },
        {
          "path": "src/theme/layer-styles.ts",
          "type": "registry:theme"
        },
        {
          "path": "src/theme/text-styles.ts",
          "type": "registry:theme"
        }
      ],
      "panda": {
        "imports": [
          {
            "type": "registry:theme",
            "from": "recipes",
            "name": "slotRecipes"
          },
          {
            "type": "registry:theme",
            "from": "recipes",
            "name": "recipes"
          },
          {
            "type": "registry:theme",
            "name": "conditions",
            "from": "conditions"
          },
          {
            "type": "registry:theme",
            "name": "globalCss",
            "from": "global-css"
          },
          {
            "type": "registry:theme",
            "name": "keyframes",
            "from": "keyframes"
          },
          {
            "type": "registry:theme",
            "name": "layerStyles",
            "from": "layer-styles"
          },
          {
            "type": "registry:theme",
            "name": "textStyles",
            "from": "text-styles"
          },
          {
            "type": "registry:theme",
            "name": "colors",
            "from": "tokens/colors"
          },
          {
            "type": "registry:theme",
            "name": "durations",
            "from": "tokens/durations"
          },
          {
            "type": "registry:theme",
            "name": "shadows",
            "from": "tokens/shadows"
          },
          {
            "type": "registry:theme",
            "name": "zIndex",
            "from": "tokens/z-index"
          },
          {
            "type": "registry:theme",
            "name": "animationStyles",
            "from": "animation-styles"
          }
        ],
        "extension": {
          "globalCss": "$ref:globalCss",
          "conditions": "$ref:conditions",
          "theme": {
            "extend": {
              "animationStyles": "$ref:animationStyles",
              "recipes": "$ref:recipes",
              "slotRecipes": "$ref:slotRecipes",
              "keyframes": "$ref:keyframes",
              "layerStyles": "$ref:layerStyles",
              "textStyles": "$ref:textStyles",
              "tokens": {
                "colors": "$ref:colors",
                "durations": "$ref:durations",
                "zIndex": "$ref:zIndex"
              },
              "semanticTokens": {
                "colors": {
                  "fg": {
                    "default": {
                      "value": {
                        "_light": "{colors.gray.12}",
                        "_dark": "{colors.gray.12}"
                      }
                    },
                    "muted": {
                      "value": {
                        "_light": "{colors.gray.11}",
                        "_dark": "{colors.gray.11}"
                      }
                    },
                    "subtle": {
                      "value": {
                        "_light": "{colors.gray.10}",
                        "_dark": "{colors.gray.10}"
                      }
                    }
                  }
                },
                "shadows": "$ref:shadows"
              }
            }
          }
        }
      }
