{
  "name": "@park-ui/panda-preset",
  "version": "0.21.0",
  "description": "Panda CSS Preset for Park UI",
  "keywords": [
    "Ark UI",
    "Panda CSS",
    "Preset",
    "Park UI"
  ],
  "author": "Christian Schr√∂ter <dev@grizzly.codes> (https://cschroeter.net/)",
  "homepage": "https://park-ui.com",
  "license": "MIT",
  "repository": "https://github.com/cschroeter/park-ui",
  "bugs": {
    "url": "https://github.com/cschroeter/park-ui/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "release-it": "release-it",
    "test": "bun test",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@ark-ui/anatomy": "0.1.0",
    "@pandacss/dev": "0.17.5",
    "deepmerge": "4.3.1",
    "ts-pattern": "5.0.5"
  },
  "devDependencies": {
    "@pandacss/types": "0.17.5",
    "@radix-ui/colors": "3.0.0",
    "@release-it/keep-a-changelog": "4.0.0",
    "clean-package": "2.2.0",
    "release-it": "16.2.1",
    "tsup": "7.2.0"
  },
  "clean-package": {
    "replace": {
      "main": "dist/index.js",
      "module": "dist/index.mjs",
      "types": "dist/index.d.ts",
      "exports": {
        ".": {
          "types": "./dist/index.d.ts",
          "import": "./dist/index.mjs",
          "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "main": "src/index.ts"
}
