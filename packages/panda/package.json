{
  "name": "@park-ui/panda-preset",
  "version": "0.42.0",
  "description": "Panda CSS Preset for Park UI",
  "keywords": ["Ark UI", "Panda CSS", "Preset", "Park UI"],
  "author": "Christian Schr√∂ter <dev@grizzly.codes> (https://cschroeter.net/)",
  "homepage": "https://park-ui.com",
  "license": "MIT",
  "repository": "https://github.com/cschroeter/park-ui",
  "bugs": {
    "url": "https://github.com/cschroeter/park-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "release-it": "release-it",
    "lint": "biome lint ./src",
    "test": "bun test",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/anatomy": "3.5.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.47.0",
    "@release-it/keep-a-changelog": "5.0.0",
    "clean-package": "2.2.0",
    "release-it": "17.10.0",
    "tsup": "8.3.5"
  },
  "peerDependencies": {
    "@pandacss/dev": ">0.22.0"
  },
  "clean-package": {
    "replace": {
      "main": "dist/index.js",
      "module": "dist/index.mjs",
      "types": "dist/index.d.ts",
      "exports": {
        ".": {
          "types": "./dist/index.d.ts",
          "import": "./dist/index.mjs",
          "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
      }
    }
  },
  "tsup": {
    "entry": ["src/index.ts", "src/colors/*.ts"],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": ["esm", "cjs"]
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./colors/*": {
      "source": "./src/colors/*.ts",
      "import": {
        "types": "./dist/colors/*.d.ts",
        "default": "./dist/colors/*.js"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    }
  }
}
